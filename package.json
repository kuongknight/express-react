{
  "name": "hello-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "better-npm-run build",
    "start": "better-npm-run start",
    "start-dev": "better-npm-run start-dev",
    "start-express": "node ./bin/www"
  },
  "betterScripts": {
    "start": {
      "command": "node ./bin/start-prd.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "start-dev": {
      "command": "node ./bin/start-prd.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3333
      }
    },
    "build": {
      "command": "webpack -p",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "better-npm-run": "0.0.12",
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "^2.5.2",
    "express": "~4.13.4",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "^3.0.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.18.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.13.1"
  }
}
